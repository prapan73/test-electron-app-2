      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "electron": "^13.1.9",
    "electron-builder": "^22.11.7",
    "parcel-bundler": "^1.12.5",
    "wait-on": "^6.0.0"
